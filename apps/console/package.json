{
  "name": "@kubelt/apps.console",
  "version": "0.1.0",
  "private": true,
  "sideEffects": true,
  "scripts": {
    "storybook:css": "npx tailwindcss -o ./app/styles/tailwind.css --watch",
    "storybook:start": "start-storybook -p 6008",
    "storybook": "run-p 'storybook:*'",
    "build-storybook": "npx tailwindcss -o ./app/styles/tailwind.css && build-storybook",
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "run-s 'build:*'",
    "build:css": "yarn run generate:css -- --minify",
    "build:remix": "remix build",
    "deploy": "wrangler publish",
    "deploy:dev": "wrangler publish --env dev",
    "dev:css": "yarn run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler dev --local --persist",
    "dev": "echo \"No dev for console. Please use start.\"",
    "start": "run-p 'dev:*'",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@headlessui/react": "1.7.5",
    "@heroicons/react": "2.0.13",
    "@kubelt/design-system": "workspace:*",
    "@kubelt/platform-clients": "workspace:*",
    "@remix-run/cloudflare": "1.11.1",
    "@remix-run/cloudflare-workers": "1.11.1",
    "@remix-run/react": "1.11.1",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@tailwindcss/forms": "0.5.3",
    "bcryptjs": "2.4.3",
    "cross-env": "7.0.3",
    "ethers": "5.7.2",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "react-icons": "4.7.1",
    "tiny-invariant": "1.3.1",
    "wagmi": "0.10.8"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@cloudflare/workers-types": "4.20221111.1",
    "@mdx-js/react": "2.1.5",
    "@remix-run/dev": "1.11.1",
    "@remix-run/eslint-config": "1.11.1",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-docs": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-webpack4": "6.5.13",
    "@storybook/manager-webpack4": "6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/testing-library": "0.0.13",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "babel-loader": "9.1.0",
    "chromatic": "6.13.1",
    "eslint": "8.28.0",
    "npm-run-all": "4.1.5",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4",
    "wrangler": "2.7.1"
  },
  "engines": {
    "node": ">=16.13"
  },
  "packageManager": "yarn@3.2.4"
}

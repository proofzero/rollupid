{
  "name": "@kubelt/services.indexer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wrangler publish --dry-run --outdir=dist",
    "db:generate": "drizzle-kit generate --dialect=sqlite --schema=src/db/schema.ts",
    "db:execute": "npx wrangler d1 execute COLLECTIONS",
    "check": "run-s format:check lint:check types:check",
    "format": "run-s format:src",
    "format:src": "prettier --write src",
    "format:check": "run-s format:check:src",
    "format:check:src": "prettier --check src",
    "lint": "eslint --fix src test",
    "lint:check": "run-s lint:check:src",
    "lint:check:src": "eslint src",
    "types:check": "tsc --project tsconfig.json",
    "services": "wrangler dev --local --persist",
    "deploy": "wrangler publish",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20221111.1",
    "@types/itty-router-extras": "0.4.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "drizzle-kit": "0.14.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "wrangler": "2.7.1"
  },
  "dependencies": {
    "@kubelt/openrpc": "workspace:*",
    "@kubelt/platform-clients": "workspace:*",
    "@kubelt/platform-middleware": "workspace:*",
    "@kubelt/urns": "workspace:*",
    "@moralisweb3/common-core": "2.10.1",
    "@moralisweb3/evm-api": "2.10.1",
    "@moralisweb3/streams": "2.10.1",
    "@trpc/server": "10.8.1",
    "drizzle-orm": "0.14.2",
    "drizzle-orm-sqlite": "0.14.2",
    "itty-router": "2.6.6",
    "itty-router-extras": "0.4.2",
    "stream": "0.0.2",
    "zod": "3.20.2"
  }
}
